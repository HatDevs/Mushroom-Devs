    else
        itemnumstring = Inventory_obj.itemnames[item] + ": " + string(item)
    draw_text(x + 15, y + 60 + (i * 15), itemnumstring)
}
if keyboard_check_pressed(ord('W')) {
    itemselected = max(itemselected - 1, 0)
}
if keyboard_check_pressed(ord('S')) {
    itemselected = min(itemselected + 1, ds_list_size(Inventory_obj.inventory))
}
</string>
